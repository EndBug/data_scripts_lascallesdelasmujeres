{
  "name": "lascallesdelasmujeres_data_scripts",
  "version": "1.0.0",
  "description": "Scripts to generate data for the geochicasOSM project, #LasCallesDeLasMujeres",
  "main": "./scripts/index.js",
  "typings": "./types/**/*.d.ts",
  "scripts": {
    "street-list": "ts-node --project tsconfig.json scripts/street-list",
    "wikipedia-step": "ts-node ./scripts/apply_wikipedia",
    "final-step": "node ./scripts/parse_final.js",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint"
  },
  "author": "Jessica Sena",
  "license": "MIT",
  "dependencies": {
    "@turf/bbox": "^6.0.1",
    "@turf/bbox-polygon": "^6.0.1",
    "@turf/boolean-contains": "^6.0.1",
    "@turf/center-of-mass": "^6.0.1",
    "@turf/flatten": "^5.1.5",
    "axios": "^1.6.2",
    "boundingbox-split": "^1.0.1",
    "csv": "^6.3.5",
    "fs": "0.0.1-security",
    "fuse.js": "^3.2.0",
    "line-by-line": "^0.1.6",
    "query-overpass": "^1.5.5",
    "split": "^1.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "wikidata-entity-lookup": "^2.1.0",
    "wikidata-entity-reader": "^4.1.1",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "@types/geojson": "^7946.0.13",
    "@types/line-by-line": "^0.1.9",
    "@types/node": "^18.18.11",
    "eslint-plugin-prettier": "^5.0.1",
    "gts": "^5.2.0",
    "husky": "^4.2.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
